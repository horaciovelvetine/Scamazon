<div class= "container py-5">
  <div class="col-md-6 offset-md-3">

  <div class="row align-items-center">
    
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="<%= profile_url %>">Profile</a></li>
      <li class="breadcrumb-item active" aria-current="page">Edit Profile</li>
    </ol>

  </div>

  <div class="d-flex flex-row pt-2 pb-0 align-items-end">
    <div class="col flex-col flex-fill">
      <h3 class="fw-light">Login & security</h3>
    </div>
  </div>

  <div class="row align-items-center">
    <div class="shadow border p-4 rounded mt-2">
      <ul class="list-group list-group-flush">
        <%= render 'devise/registrations/shared/edit_form' %>
      </ul>
    </div>
  </div>
  <small>
    <div class="text-muted m-0 p-0">
      Unhappy? <%= link_to "D", registration_path(resource_name), data: { confirm: "You cannot escape the Scamazon Corporation. Go ahead. See if we care." }, method: :delete, class:" text-decoration-none" %><%= link_to "elete My Account", root_path, class:" text-decoration-none" %>
    </div>
  </small>
  <div class="mt-3"><%= link_to "Back", :back, class:"fs-5" %></div>
</div>










<div class="container mt-5 pt-5">

</div>